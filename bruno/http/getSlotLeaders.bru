meta {
  name: getSlotLeaders
  type: http
  seq: 35
}

post {
  url: {{baseUrl}}
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "jsonrpc": "2.0",
    "id": 1,
    "method": "getSlotLeaders",
    "params": [
      {{startSlot}},
      {{limit}}
    ]
  }
}

vars:pre-request {
  startSlot: 100
  limit: 10
}

settings {
  encodeUrl: false
  timeout: 0
}
